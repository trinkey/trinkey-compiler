end:||----
end:|----
?__:| $P == 4 and $4 == "4":
?__:|| $T == 0:
str:||| "X", $4
:__:|| ELSE:
str:||| "O", $4
end:||----
end:|----
?__:| $P == 5 and $5 == "5":
?__:|| $T == 0:

dsp: "Tic Tac Toe!"
dsp: "1: Memory Mode"
dsp: "2: Normal Mode"
in_: ">> ", $W
?__: $W == 1:
str:| 69, $W
:__: ELSE:
str:| 0, $W
end:----
str: "1", $1
str: "2", $2
str: "3", $3
str: "4", $4
str: "5", $5
str: "6", $6
str: "7", $7
str: "8", $8
str: "9", $9
str: 0, $H
str: 0, $T
whl: ($1 == "1" or $2 == "2" or $3 == "3" or $4 == "4" or $5 == "5" or $6 == "6" or $7 == "7" or $8 == "8" or $9 == "9") and $H == 0:
?__:| $W != 69:
dsp:|| $7 + "I" + $8 + "I" + $9
dsp:|| "-+-+-"
dsp:|| $4 + "I" + $5 + "I" + $6
dsp:|| "-+-+-"
dsp:|| $1 + "I" + $2 + "I" + $3
end:|----
?__:| $T == 0:
dsp:|| "X turn!"
:__:| ELSE:
dsp:|| "O turn!"
end:|----
?__:| $W == 69:
in_:|| "",$P
:__:| ELSE:
in_:|| "Where do you want to play?",$P
end:|----
?__:| $P == 1 and $1 == "1":
?__:|| $T == 0:
str:||| "X", $1
:__:|| ELSE:
str:||| "O", $1
end:||----
end:|----
?__:| $P == 2 and $2 == "2":

end:||----
end:|----
?__:| $P == 9 and $9 == "9":
?__:|| $T == 0:
str:||| "X", $9
:__:|| ELSE:
str:||| "O", $9
end:||----
end:|----
add:| $T, 1, $T
?__:| $T == 2:
str:|| 0, $T
end:|----
?__:| $W == 69:
dsp:|| "Tic Tac Toe!"
end:|----
?__:| ($1 == $2 and $2 == $3) or ($4 == $5 and $5 == $6) or ($7 == $8 and $8 == $9) or ($1 == $4 and $4 == $7) or ($2 == $5 and $5 == $8) or ($3 == $6 and $6 == $9) or ($1 == $5 and $5 == $9) or ($7 == $5 and $5 == $3):
str:|| 1, $H
?__:|| $1 == "1":
str:||| " ", $1
end:||----
